<%= form_for @lab do |f| %>

  <div class="field">
    <%= f.label :lab_name %>
    <%= f.text_field :lab_name %>
  </div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label :starting_date %>
    <%= f.text_field :starting_date %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
